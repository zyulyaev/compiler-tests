fun printArray (A: [Int]): None
begin
  for i:=0, i<arrlen (A), i:=i+1
  do
    write (A[i])
  od;
end

fun readArray (A: [Int]): [Int]
begin
  for i:=0, i<arrlen (A), i:=i+1
  do
    A[i] := read ()
  od;
  return A
end

A := arrmake (5, 0); 
A := readArray (A);
printArray (A);

A := [1, 2, 3, 4, 5];
printArray (A)

